{
    "name": "touhidurabir/laravel-model-uuid",
    "description": "A laravel package to attach uuid to model classes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Touhidur Rahman",
            "email": "abircse06@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Touhidurabir\\ModelUuid\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Touhidurabir\\ModelUuid\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.20",
        "illuminate/support": "^8.54",
        "illuminate/container": "^8.54",
        "illuminate/database": "^8.54",
        "illuminate/events": "^8.54"
    },
    "require": {
        "ramsey/uuid": "^4.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Touhidurabir\\ModelUuid\\ModelUuidServiceProvider"
            ],
            "aliases": {
                "ModelUuid": "Touhidurabir\\ModelUuid\\Facades\\ModelUuid"
            }
        }
    }
}
